package com.zhjy.hdcivilization.entity;

/**
 * @author :huangxianfeng on 2016/7/25.
 * （建表）轮播图实体的定义
 */
public class HDC_Carousel extends EntityBase{

    private String itemId;//item的Id
    private String  imgDes;//图片描述
    private String imgUrl;//图片Url
    private String imgThumbUrl;//缩略图

    public HDC_Carousel(String itemId, String imgDes, String imgUrl, String imgUrls) {
        this.itemId = itemId;
        this.imgDes = imgDes;
        this.imgUrl = imgUrl;
        this.imgThumbUrl = imgUrls;
    }

    public String getItemId() {
        return itemId;
    }

    public void setItemId(String itemId) {
        this.itemId = itemId;
    }

    public String getImgDes() {
        return imgDes;
    }

    public void setImgDes(String imgDes) {
        this.imgDes = imgDes;
    }

    public String getImgUrl() {
        return imgUrl;
    }

    public void setImgUrl(String imgUrl) {
        this.imgUrl = imgUrl;
    }


    public String getImgUrls() {
        return imgThumbUrl;
    }

    public void setImgUrls(String imgUrls) {
        this.imgThumbUrl = imgUrls;
    }

    public HDC_Carousel() {
    }

    @Override
    public String toString() {
        return "HDC_Carousel{" +
                "itemId='" + itemId + '\'' +
                ", imgDes='" + imgDes + '\'' +
                ", imgUrl='" + imgUrl + '\'' +
                ", imgUrls='" + imgThumbUrl + '\'' +
                '}';
    }
}
