<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ebf8ff"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:layout_marginBottom="20dp"
        android:background="@color/title_backround_color">

        <RelativeLayout
            android:id="@+id/rl_back"
            android:layout_width="@dimen/back_height"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/btn_back"
                style="@style/TitleLeftBtn"
                android:background="@null"
                android:contentDescription="@string/empty_space"
                android:src="@drawable/btn_back_selector" />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/apply_check"
            android:textColor="@color/title_text_color"
            android:textSize="@dimen/title_text_size" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/apply_no_pass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="visible">

        <RelativeLayout
            android:id="@+id/rl_apply_1"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/mine_apply_check_bg">


            <TextView
                android:id="@+id/date_apply_check_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:text="8月30日"
                android:textColor="#4a4a4a"
                android:textSize="17sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/date_apply_check_1"
                android:layout_below="@id/date_apply_check_1"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_apply_check_1"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:background="@drawable/mine_apply_check_success" />

                <TextView
                    android:id="@+id/tv_apply_time_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:lines="2"
                    android:layout_marginRight="40dp"
                    android:textColor="@color/black"
                    android:textSize="14sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/apply_time_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/date_apply_check_1"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:textColor="#bababa"
                android:textSize="14sp" />

        </RelativeLayout>

        <ImageView
            android:layout_width="@dimen/apply_check_sign_wight"
            android:layout_height="@dimen/apply_check_sign_height"
            android:layout_marginLeft="14dp"
            android:background="@drawable/mine_apply_check_sign" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/img_line1"
        android:layout_width="5dp"
        android:layout_height="25dp"
        android:layout_marginLeft="23dp"
        android:src="@drawable/mine_apply_check"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/apply_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/rl_apply_2"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/mine_apply_check_bg">

            <TextView
                android:id="@+id/date_apply_check_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:text="8月30日"
                android:textColor="#4a4a4a"
                android:textSize="17sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/date_apply_check_2"
                android:layout_below="@id/date_apply_check_2"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_apply_check_2"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:background="@drawable/mine_apply_check_success" />

                <TextView
                    android:id="@+id/tv_apply_time_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="恭喜您，审核通过"
                    android:textColor="#3da271"
                    android:textSize="14sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/apply_time_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/date_apply_check_2"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:text="10:27 AM"
                android:textColor="#bababa"
                android:textSize="14sp" />

        </RelativeLayout>

        <ImageView
            android:layout_width="@dimen/apply_check_sign_wight"
            android:layout_height="@dimen/apply_check_sign_height"
            android:layout_marginLeft="14dp"
            android:background="@drawable/mine_apply_check_sign" />

    </RelativeLayout>


    <ImageView
        android:id="@+id/img_line2"
        android:layout_width="5dp"
        android:layout_height="25dp"
        android:layout_marginLeft="23dp"
        android:src="@drawable/mine_apply_check"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/apply_applying"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/rl_apply_3"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/mine_apply_check_bg">

            <TextView
                android:id="@+id/date_apply_check_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:text="8月30日"
                android:textColor="#4a4a4a"
                android:textSize="17sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/date_apply_check_3"
                android:layout_below="@id/date_apply_check_3"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_apply_check_3"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:background="@drawable/mine_apply_checking" />

                <TextView
                    android:id="@+id/tv_apply_time_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="您提交的申请已经开始审核"
                    android:textColor="#f88d5c"
                    android:textSize="14sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/apply_time_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/date_apply_check_3"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:text="8:30 AM"
                android:textColor="#bababa"
                android:textSize="14sp" />

        </RelativeLayout>

        <ImageView
            android:layout_width="@dimen/apply_check_sign_wight"
            android:layout_height="@dimen/apply_check_sign_height"
            android:layout_marginLeft="14dp"
            android:background="@drawable/mine_apply_check_sign" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/img_line3"
        android:layout_width="5dp"
        android:layout_height="25dp"
        android:layout_marginLeft="23dp"
        android:src="@drawable/mine_apply_check"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/sub_apply"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/rl_apply_4"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/mine_apply_check_bg">

            <TextView
                android:id="@+id/date_apply_check_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:text="8月30日"
                android:textColor="#4a4a4a"
                android:textSize="17sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/date_apply_check_4"
                android:layout_below="@id/date_apply_check_4"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/img_apply_check_4"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:background="@drawable/mine_apply_" />

                <TextView
                    android:id="@+id/tv_apply_time_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="您申请已提交成功"
                    android:textColor="#59bcfa"
                    android:textSize="14sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/apply_time_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/date_apply_check_4"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:text="10:24 AM"
                android:textColor="#bababa"
                android:textSize="14sp" />

        </RelativeLayout>

        <ImageView
            android:layout_width="@dimen/apply_check_sign_wight"
            android:layout_height="@dimen/apply_check_sign_height"
            android:layout_marginLeft="14dp"
            android:background="@drawable/mine_apply_check_sign" />
    </RelativeLayout>


</LinearLayout>
