<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.zhjy.hdcivilization.view.SimpleSwipeRefreshLayout
        android:id="@+id/mine_swipe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.zhjy.hdcivilization.view.LoadMoreRecyclerView
            android:id="@+id/mine_recyclerview"
            android:layout_width="match_parent"
            android:scrollbarStyle="outsideOverlay"
            android:scrollbarThumbVertical="@drawable/temp_scrollbars_outside_1"
            android:scrollbars="vertical"
            android:layout_height="wrap_content" />

    </com.zhjy.hdcivilization.view.SimpleSwipeRefreshLayout>


</FrameLayout>
