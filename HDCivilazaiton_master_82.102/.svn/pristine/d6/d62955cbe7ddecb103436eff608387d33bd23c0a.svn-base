package com.zhjy.hdcivilization.entity;

import com.lidroid.xutils.db.annotation.Id;

/**
 * 
 * @author zhangyang
 * 
 * @日期 2016-3-19上午9:21:38
 * 
 * @描述   数据库实体的基础类
 */
public abstract class EntityBase {
    @Id(column="id")
	private int id;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	@Override
	public String toString() {
		return "EntityBase{" +
				"id=" + id +
				'}';
	}
}
